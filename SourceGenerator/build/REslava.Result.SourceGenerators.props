<Project>
  <PropertyGroup>
    <!-- Mark this as a source generator for better IDE support -->
    <IsRoslynComponent>true</IsRoslynComponent>
  </PropertyGroup>
  
  <!-- Ensure the generator is loaded -->
  <ItemGroup>
    <Analyzer Include="$(MSBuildThisFileDirectory)..\analyzers\dotnet\cs\REslava.Result.SourceGenerators.dll" />
  </ItemGroup>
</Project>
